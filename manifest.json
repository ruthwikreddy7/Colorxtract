{
  "name": "Colorxtract",
  "description": "With Colorxtract, you can select the color you desire from websites",
  "version": "0.0.1",
  "manifest_version": 3,
  "action": {
      "default_popup": "./popup.html"
  },
  "content_scripts": [
      {
          "matches": [
              "<all_urls>"
          ],
          "js": [
              "./contentScript.js"
          ]
      }
  ],
  "permissions": [
      "tabs",
      "storage"
  ],
  "host_permissions": [
      "<all_urls>"
  ],
  "icons" :{
    "16" :"images/logo.png",
    "32" :"images/logo.png",
    "48" :"images/logo.png"
  }
}